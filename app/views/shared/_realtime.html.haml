.panel-group#accordian
  .panel.panel-default
    .panel-heading
      %h4.panel-title
        %a{'data-toggle'=>'collapse', 'data-parent'=>'#accordion', 'href'=>'#collapse1'} Realtime
    #collapse1.panel-collapse.collapse.in
      .panel-body
        -# %h6 'R:#{@pick.round} | P:#{@pick.pick}'

        .col-md-4
          %h5.recent_picks Most Recent Picks
          %span.recent-picks-container
            -if @recent_picks.count > 0
              -@recent_picks.each do |p|
                %p #{p.team.tname} drafted #{p.player.position} #{p.player.pname}
            -else
              %h6 None Available
        .col-md-4
          %h5 Picking Now
          %p.picking_now #{@picking_now.team.tname}
        .col-md-4
          %h5 Next Pick
          %p.picking_next #{@picking_next.team.tname}
