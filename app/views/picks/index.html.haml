-if @draft_status == 'open'
  #pick-dashboard
    %h3 Draft Stats Dashboard

    = render :partial => 'shared/realtime'
    = render :partial => 'shared/round_picks'
    = render :partial => 'shared/team_picks'
    = render :partial => 'shared/available_players'

-elsif @draft_status == 'completed'
  = render :partial => 'shared/draft_final'
-else
  %h4 Draft data has not been loaded
